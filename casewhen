set serveroutput on;
 declare
 grd char:='&new_grad';
 begin
 case grd
 when 'A' then dbms_output.put_line('outstanding');
 when 'B' then dbms_output.put_line('outsitting');
 when 'C' then dbms_output.put_line('outdancing');
 when 'D' then dbms_output.put_line('outflying');
 when  'E' then dbms_output.put_line('outraing');
 when 'F' then dbms_output.put_line('its okay try again');
 else dbms_output.put_line('No such grade'); 
end case;
end;
